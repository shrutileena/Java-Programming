import java.util.*;

class stringConcat {
    public static void main(String[] args) {
        
        String name = "Shruti";
        name.concat(" Agarwal");
        
        System.out.println(name);
        
        String name1 = name.concat(" Agarwal");
        System.out.println(name1);
        
        String name2 = "Shruti";
        name2 = name2.concat(" Agarwal");
         System.out.println(name2);
        
    }
}
